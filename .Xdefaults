URxvt.geometry: 115x35

URxvt.saveLines: 10000
URxvt.scrollBar: false
!URxvt.foreground: white
!URxvt.background: black
!URxvt.shading: 10
!URxvt.background: rgba:0000/0000/0000/baba
!URxvt.depth: 32
URxvt.secondaryScroll: true
URxvt.font: xft:Monospace:pixelsize=10:antialias=false
