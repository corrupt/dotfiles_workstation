!! urxvt config {{{

URxvt.geometry: 115x35

URxvt.saveLines: 10000
URxvt.scrollBar: false
!URxvt.foreground: white
!URxvt.background: black

!! Real transparency
URxvt.background: rgba:0000/0000/0000/a0a0
URxvt.depth: 32

!! Fake transparency
!URxvt*transparent: true
!URxvt*shading: 40
!URxvt*inheritPixmap: true
!URxvt.tint: white

URxvt*secondaryScroll: true
!URxvt*font: xft:Monospace:pixelsize=10:antialias=false
!URxvt.font: xft:Bitstream Vera Sans Mono:pixelsize=10
URxvt*perl-ext-common: default,matcher,tabbed
URxvt*urlLauncher: firefox
URxvt*matcher.button: 1
URxvt*iconFile: /usr/kde/4.2/share/icons/oxygen/16x16/apps/terminal.png
URxvt*urgentOnBell: true


!! with tabs:
URxvt.tabbed.saveLines: 10000
URxvt.tabbed.scrollBar: false
URxvt.tabbed.tab-fg: 8
URxvt.tabbed.tab-bg: 12
URxvt.tabbed.tabbar-fg: 12
URxvt.tabbed.tabbar-bg: 8


